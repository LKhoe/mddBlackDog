datatype String

entity Organization {
    name: String
    cnpj: String
}

entity Project {
    name: String
    org: Organization
}

@None
entity Team {
    name: String
}

entity ProjectTeam extends Team {
    project: Project
}

@None
entity Person {
    name: String
    cpf: String
}

entity TeamMember extends Person {
    team: Team
}
